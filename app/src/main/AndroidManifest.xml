    package="com.example.suwan.godrivefinalproject">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/godrivelogo"
        android:label="GoDrive"
        android:roundIcon="@drawable/godrivelogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".WhereActivity" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAeh41qLHFdZup23jZJRfDW0-073aJn6yA" />

        <activity android:name=".DestinationActivity" />
        <activity android:name=".ListActivity" />
        <activity android:name=".DriverActivity" />
